byte b = 1 // от -128 до 127 8 bit
short s = 2 // от -32768 до 32767 16 bit
int i = 3 // от -2147483648 до 2147483647 32 bit
long l = 4L // от -9223372036854775808 до 9223372036854775807 64 bit
float f = 12.43f //  32 битное число с плавающей запятой
double d = 134.6432 // 64 битное число с плавающей запятой
char c = 'cnf8423!' // 16 битный тип символов Unicode
boolean bul  = true или false